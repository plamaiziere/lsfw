interface FastEthernet1/0
 description INTERNET
 ip address 10.0.3.1 255.255.255.0
interface FastEthernet1/1
 description inter R1-R2
 ip address 10.0.1.254 255.255.255.0
interface FastEthernet1/2
 description NET2
 ip address 10.0.2.254 255.255.255.0
ip route 192.168.10.0 255.255.255.0 10.0.2.1
ip route 192.168.11.0 255.255.255.0 10.0.2.1
ip route 192.168.12.0 255.255.255.0 10.0.2.1
ip route 192.168.0.0 255.255.255.0 10.0.1.1
ip route 192.168.1.0 255.255.255.0 10.0.1.1
ip route 192.168.2.0 255.255.255.0 10.0.1.1
